{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "no-extra-semi": 1,
    "semi": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-console": 0,
    "prefer-destructuring": 0,
    "no-underscore-dangle": [0]
  },
  "extends": ["airbnb"]
}