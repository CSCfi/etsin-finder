  "scripts": {
    "build": "webpack --mode production --config webpack.prod.js --define process.env.NODE_ENV=\"'production'\"",
    "build-storybook": "build-storybook",
    "dev-build": "webpack -d --config webpack.config.js --define process.env.NODE_ENV=\"'development'\"",
    "lint": "eslint js --ext .jsx,.js",
    "start": "webpack --progress -d --config webpack.config.js --watch  --define process.env.NODE_ENV=\"'development'\"",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "watch": "npm update; webpack --progress -d --config webpack.config.js --watch  --define process.env.NODE_ENV=\"'development'\""
  },
