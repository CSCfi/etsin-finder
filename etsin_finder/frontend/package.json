{
  "name": "etsin-finder",
  "version": "1.0.0",
  "description": "Research data finder",
  "main": "./static/index.html",
  "author": "Aaron Hakala",
  "repository": {
    "type": "git",
    "url": "https://github.com/CSCfi/etsin-finder"
  },
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode production --config webpack.prod.js --define process.env.NODE_ENV=\"'production'\"",
    "build-storybook": "build-storybook",
    "dev-build": "webpack -d --config webpack.config.js --define process.env.NODE_ENV=\"'development'\"",
    "lint": "eslint js --ext .jsx,.js",
    "start": "webpack --progress -d --config webpack.config.js --watch --define process.env.NODE_ENV=\"'development'\"",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "dev": "webpack-dev-server --config webpack.dev.js --inline --define process.env.NODE_ENV=\"'development'\"",
    "watch": "npm update; webpack --progress -d --config webpack.config.js --watch  --define process.env.NODE_ENV=\"'development'\""
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "axios": "0.19.0",
    "bluebird": "3.5.3",
    "clean-webpack-plugin": "^2.0.2",
    "counterpart": "0.18.6",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "formik": "^1.5.8",
    "html-webpack-plugin": "^3.2.0",
    "leaflet": "^1.6.0",
    "leaflet-geosearch": "2.7.0",
    "mobx": "^5.15.4",
    "mobx-react": "^5.4.4",
    "mobx-react-router": "^4.1.0",
    "moment": "^2.25.3",
    "moment-parseformat": "^3.0.0",
    "polished": "^3.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-clipboard-icon": "1.1.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-interpolate-component": "^0.12.0",
    "react-leaflet": "2.1.2",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^2.4.4",
    "react-transition-group": "^2.9.0",
    "react-translate-component": "0.15.1",
    "react-with-direction": "^1.3.1",
    "style-loader": "^0.23.1",
    "styled-components": "4.2.0",
    "terraformer-wkt-parser": "1.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "history": "4.7.2",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "react-axe": "^3.4.1",
    "react-element-to-jsx-string": "^14.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/test-setup.js"
    ],
    "moduleNameMapper": {
      "Components/(.*)$": "<rootDir>/js/components/$1",
      "Stores/(.*)$": "<rootDir>/js/stores/$1",
      "Utils/(.*)$": "<rootDir>/js/utils/$1",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "./__tests__/test-setup.js",
      "./__tests__/__testdata__"
    ],
    "testURL": "http://localhost"
  },
  "prettier": {
    "function-paren-newline": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
